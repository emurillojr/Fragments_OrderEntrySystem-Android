<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="neit.lab1fragments.MainActivity"
    tools:showIn="@layout/activity_main">

    <fragment
        android:layout_width="454dp"
        android:layout_height="338dp"
        android:name="neit.lab1fragments.BlankFragment"
        android:id="@+id/blankfragment"
        tools:layout="@layout/fragment_blank"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="4dp"
        android:layout_alignLeft="@+id/ItemFragment"
        android:layout_alignStart="@+id/ItemFragment" />


    <fragment
        android:layout_width="400dp"
        android:layout_height="244dp"
        android:name="neit.lab1fragments.ItemFragment"
        android:id="@+id/ItemFragment"
        android:layout_below="@+id/blankfragment"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:layout="@layout/fragment_item_list"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="5dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_alignParentBottom="true" />

</RelativeLayout>
